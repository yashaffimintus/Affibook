  <body class="signpage">

    <div class="container">
   
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name),html: { multipart: true }) do |f| %>
      <%= devise_error_messages! %>
      <div class="box-header">
        <h2>Log In</h2>
        </div>
  <div class="field">
  
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name,:min => "4",:maxlength => "40",:autofocus => true,:placeholder=> "Enter Firstname", :required => true, :value => "" %>
  <!-- </div><div id="demo"></div> -->
  
  <div class="field">
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name,:placeholder=> "Enter Lastname", :min => "4",:max => "40",:autofocus => true, :required => true, :value => "" %>
  <!-- </div><div id="demo1"></div> -->
  <div class="field">
    <label>My Avatar</label><br />
    <%= image_tag(@user.avatar_url) if @user.avatar? %>
    <%= f.label :avatar do %>
    <%= f.file_field :avatar,:require => true %>
    <%= f.hidden_field :avatar_cache %>
    <% end %>
  </div>
  <div class="field">
    <%= f.label :role %><br />
    <%= f.select :role, collection: User.roles.keys.to_a %>
  </div>
   
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, :min => "4",:max => "10",:autofocus => true, :required => true,:value => "" %>
  <!-- </div><div id="demo2"></div> -->
  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off", :require => true %>
  </div>
  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation,:placeholder=> "Re-enter Password", autocomplete: "off", :require => true %>
  </div>
            <label class="checkbox">
                <input type="checkbox" value="agree this condition"> I agree to the Terms of Service and Privacy Policy
            </label>
  <div class="actions">
    <%= f.submit "Submit",class: "btn btn-lg btn-login btn-block",:onclick => "myFunction()"%>
  </div>
<% end %>
  <div class="registration">
      Already Registered.
      <a class="" href="/users/sign_in">
          Login
      </a>
  </div>
</body>
 </div>


  </body>
</html> 

      


  


    


